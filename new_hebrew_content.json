[
    {
        "text": "אריה",
        "lang": "he"
    },
    {
        "text": "האריה הוא מלך החיות.",
        "lang": "he"
    },
    {
        "text": "פיל",
        "lang": "he"
    },
    {
        "text": "לפיל יש חדק ארוך מאוד.",
        "lang": "he"
    },
    {
        "text": "ג'ירפה",
        "lang": "he"
    },
    {
        "text": "הג'ירפה אוכלת עלים מהעץ הגבוה.",
        "lang": "he"
    },
    {
        "text": "קוף",
        "lang": "he"
    },
    {
        "text": "הקוף אוהב לקפוץ על העצים.",
        "lang": "he"
    },
    {
        "text": "זברה",
        "lang": "he"
    },
    {
        "text": "לזברה יש פסים בשחור ולבן.",
        "lang": "he"
    },
    {
        "text": "נחש",
        "lang": "he"
    },
    {
        "text": "הנחש זוחל על האדמה בשקט.",
        "lang": "he"
    },
    {
        "text": "ציפור",
        "lang": "he"
    },
    {
        "text": "הציפור בנתה קן על העץ.",
        "lang": "he"
    },
    {
        "text": "דג",
        "lang": "he"
    },
    {
        "text": "הדג שוחה במים הכחולים.",
        "lang": "he"
    },
    {
        "text": "סוס",
        "lang": "he"
    },
    {
        "text": "הסוס דוהר מהר בשדה.",
        "lang": "he"
    },
    {
        "text": "כבשה",
        "lang": "he"
    },
    {
        "text": "לכבשה יש צמר רך ונעים.",
        "lang": "he"
    },
    {
        "text": "לחם",
        "lang": "he"
    },
    {
        "text": "אמא קנתה לחם טרי במאפייה.",
        "lang": "he"
    },
    {
        "text": "חלב",
        "lang": "he"
    },
    {
        "text": "אני שותה חלב חם לפני השינה.",
        "lang": "he"
    },
    {
        "text": "גבינה",
        "lang": "he"
    },
    {
        "text": "שמתי גבינה צהובה בתוך הכריך.",
        "lang": "he"
    },
    {
        "text": "ביצה",
        "lang": "he"
    },
    {
        "text": "אכלתי ביצה קשה לארוחת בוקר.",
        "lang": "he"
    },
    {
        "text": "תפוח",
        "lang": "he"
    },
    {
        "text": "התפוח האדום מתוק וטעים.",
        "lang": "he"
    },
    {
        "text": "בננה",
        "lang": "he"
    },
    {
        "text": "הקוף קילף את הבננה ואכל אותה.",
        "lang": "he"
    },
    {
        "text": "ענבים",
        "lang": "he"
    },
    {
        "text": "הענבים הירוקים גדלים בכרם.",
        "lang": "he"
    },
    {
        "text": "תפוז",
        "lang": "he"
    },
    {
        "text": "סחטתי מיץ מתפוז טרי.",
        "lang": "he"
    },
    {
        "text": "עוגה",
        "lang": "he"
    },
    {
        "text": "ביום ההולדת אכלנו עוגה גדולה.",
        "lang": "he"
    },
    {
        "text": "גלידה",
        "lang": "he"
    },
    {
        "text": "בקיץ כיף לאכול גלידה קרה.",
        "lang": "he"
    },
    {
        "text": "חולצה",
        "lang": "he"
    },
    {
        "text": "לבשתי חולצה לבנה לחג.",
        "lang": "he"
    },
    {
        "text": "מכנסיים",
        "lang": "he"
    },
    {
        "text": "המכנסיים החדשים שלי בצבע כחול.",
        "lang": "he"
    },
    {
        "text": "שמלה",
        "lang": "he"
    },
    {
        "text": "הילדה לבשה שמלה יפה למסיבה.",
        "lang": "he"
    },
    {
        "text": "מעיל",
        "lang": "he"
    },
    {
        "text": "בחורף אני לובש מעיל מחמם.",
        "lang": "he"
    },
    {
        "text": "נעליים",
        "lang": "he"
    },
    {
        "text": "קשרתי את השרוכים בנעליים שלי.",
        "lang": "he"
    },
    {
        "text": "גרביים",
        "lang": "he"
    },
    {
        "text": "הגרביים שומרים על הרגליים חמות.",
        "lang": "he"
    },
    {
        "text": "כובע",
        "lang": "he"
    },
    {
        "text": "חבשתי כובע כדי להגן מפני השמש.",
        "lang": "he"
    },
    {
        "text": "צעיף",
        "lang": "he"
    },
    {
        "text": "הצעיף הצבעוני עוטף את הצוואר.",
        "lang": "he"
    },
    {
        "text": "מגפיים",
        "lang": "he"
    },
    {
        "text": "נעלתי מגפיים וקפצתי בשלוליות.",
        "lang": "he"
    },
    {
        "text": "סוודר",
        "lang": "he"
    },
    {
        "text": "הסוודר הסרוג נעים מאוד למגע.",
        "lang": "he"
    },
    {
        "text": "ראש",
        "lang": "he"
    },
    {
        "text": "חבשתי כובע על הראש.",
        "lang": "he"
    },
    {
        "text": "יד",
        "lang": "he"
    },
    {
        "text": "אני כותב ביד ימין במחברת.",
        "lang": "he"
    },
    {
        "text": "רגל",
        "lang": "he"
    },
    {
        "text": "בעטתי בכדור ברגל חזקה.",
        "lang": "he"
    },
    {
        "text": "בטן",
        "lang": "he"
    },
    {
        "text": "כואבת לי הבטן כי אכלתי יותר מדי.",
        "lang": "he"
    },
    {
        "text": "גב",
        "lang": "he"
    },
    {
        "text": "ילקוט בית הספר מונח על הגב שלי.",
        "lang": "he"
    },
    {
        "text": "עיניים",
        "lang": "he"
    },
    {
        "text": "עצמתי עיניים והלכתי לישון.",
        "lang": "he"
    },
    {
        "text": "אוזניים",
        "lang": "he"
    },
    {
        "text": "אני שומע מוזיקה באוזניים.",
        "lang": "he"
    },
    {
        "text": "פה",
        "lang": "he"
    },
    {
        "text": "פתחתי פה גדול אצל רופא השיניים.",
        "lang": "he"
    },
    {
        "text": "אף",
        "lang": "he"
    },
    {
        "text": "יש לי נזלת באף כי אני מצונן.",
        "lang": "he"
    },
    {
        "text": "שיניים",
        "lang": "he"
    },
    {
        "text": "אני מצחצח שיניים בבוקר ובערב.",
        "lang": "he"
    },
    {
        "text": "בית ספר",
        "lang": "he"
    },
    {
        "text": "אני הולך לבית ספר כל בוקר.",
        "lang": "he"
    },
    {
        "text": "גן משחקים",
        "lang": "he"
    },
    {
        "text": "אחר הצהריים ניפגש בגן המשחקים.",
        "lang": "he"
    },
    {
        "text": "סופרמרקט",
        "lang": "he"
    },
    {
        "text": "הלכנו לסופרמרקט לקנות מצרכים.",
        "lang": "he"
    },
    {
        "text": "ים",
        "lang": "he"
    },
    {
        "text": "בקיץ אנחנו נוסעים לשחות בים.",
        "lang": "he"
    },
    {
        "text": "בריכה",
        "lang": "he"
    },
    {
        "text": "למדתי לשחות בבריכה העירונית.",
        "lang": "he"
    },
    {
        "text": "ספרייה",
        "lang": "he"
    },
    {
        "text": "השאלתי ספר מעניין מהספרייה.",
        "lang": "he"
    },
    {
        "text": "קולנוע",
        "lang": "he"
    },
    {
        "text": "ראינו סרט מצחיק בקולנוע.",
        "lang": "he"
    },
    {
        "text": "פארק",
        "lang": "he"
    },
    {
        "text": "עשינו פיקניק משפחתי בפארק הגדול.",
        "lang": "he"
    },
    {
        "text": "בית חולים",
        "lang": "he"
    },
    {
        "text": "הרופאים עוזרים לחולים בבית החולים.",
        "lang": "he"
    },
    {
        "text": "מסעדה",
        "lang": "he"
    },
    {
        "text": "אכלנו פיצה טעימה במסעדה.",
        "lang": "he"
    }
]